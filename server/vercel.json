{
   "version": 2,
   "builds": [
      {
         "src": "./index.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/api/login/forgotten-password",
         "methods": ["GET", "POST", "OPTIONS"],
         "headers": {
            "Access-Control-Allow-Origin": "https://dedoexpress.vercel.app",
            "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
            "Access-Control-Allow-Headers": "Content-Type"
         },
         "dest": "/"
      },
      {
         "src": "/(.*)",
         "dest": "/"
      }
   ]
}
